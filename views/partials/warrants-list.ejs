<div class="flex justify-between items-center mb-4">
    <h2 class="text-xl font-bold">Warrants</h2>
</div>
<% if (caseData.warrants && caseData.warrants.length > 0) { %>
  <ul class="divide-y divide-gray-200">
    <% caseData.warrants.forEach(w => { %>
      <li class="py-2"><%= w.status %>: <%= w.details %></li>
    <% }) %>
  </ul>
<% } else { %>
  <div class="text-center py-8 bg-gray-50 rounded-lg">
    <p class="text-gray-500">No warrants have been issued yet.</p>
  </div>
<% } %>
