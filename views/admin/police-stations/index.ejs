<h1 class="text-2xl font-bold mb-4">Manage Police Stations</h1>

<a href="/admin/police-stations/new" class="bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded mb-4 inline-block">New Police Station</a>

<table class="min-w-full bg-white">
  <thead>
    <tr>
      <th class="py-2 px-4 border-b">ID</th>
      <th class="py-2 px-4 border-b">Name</th>
      <th class="py-2 px-4 border-b">City</th>
    </tr>
  </thead>
  <tbody>
    <% policeStations.forEach(station => { %>
      <tr>
        <td class="py-2 px-4 border-b"><%= station.id %></td>
        <td class="py-2 px-4 border-b"><%= station.name %></td>
        <td class="py-2 px-4 border-b"><%= station.cityId %></td>
      </tr>
    <% }) %>
  </tbody>
</table>
